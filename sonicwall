SSLVPN_BASE_EVENT id=%{WORD:device} sn=%{WORD:serialnumber} time=%{QUOTEDSTRING} vp_time=%{QUOTEDSTRING} fw=%{IPORHOST:fwaddr} pri=%{INT} m=%{INT} c=%{INT} src=%{IPORHOST:sourceaddr} dst=%{IPORHOST:destinationaddr} user=%{QUOTEDSTRING:username} usr=%{QUOTEDSTRING} msg=%{QUOTEDSTRING:msg}

SSLVPN_USER_EVENT %{SSLVPN_BASE_EVENT} rule=%{NOTSPACE:rule} proto=%{WORD:protocol} agent=%{QUOTEDSTRING:agent}
